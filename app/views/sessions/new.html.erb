<h2>Log In</h2>

<%= form_tag log_in_path do %>
  <%= label_tag :email %> <br>
  <%= text_field_tag :email, params[:email]%>
  <br>
  <%= label_tag :username %> <br>
  <%= text_field_tag :username, params[:username]%>
  <br>
  <%= label_tag :password %> <br>
  <%= password_field_tag :password %>
  <%= submit_tag "Log In" %>
<% end %>
